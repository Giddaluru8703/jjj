	public void bmethod(){
		System.out.println("openBrowser,url,Login");
	}
	@AfterMethod
	public void amethod(){
		System.out.println("Logout,closeBrowser");
	}
	@Test
	public void testCase2(){
		//selenium code to savemail
		System.out.println("savemail");
	}
	@Test
	public void testCase3(){
